<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jowang.androidnoterecycler817">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NoteActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"/>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
        <activity android:name=".NoteApp.AddNote" android:parentActivityName=".NoteActivity"></activity>
        <activity android:name=".NoteApp.Detail" android:parentActivityName=".NoteActivity"></activity>
        <activity android:name=".NoteApp.Edit" android:parentActivityName=".NoteActivity"></activity>
    </application>


</manifest>